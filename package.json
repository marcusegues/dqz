{
  "name": "dazit",
  "version": "1.0.5",
  "description": "QuickZoll App",
  "author": "ambrite ag 2018",
  "license": "private",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "lint:tough": "eslint src --max-warnings=0",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:coverage:ci": "jest --coverage --maxWorkers 2",
    "test:coverage:watch": "jest --watchAll --coverage",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js' -x 'src/**/__tests__/**' -t html -t json -t text --threshold 90",
    "doc:md": "documentation build src/** -f md -o doc/apiDoc.md",
    "doc:html": "documentation build src/** -f html -o docs"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "preset": "jest-expo",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ]
  },
  "dependencies": {
    "@expo/samples": "^2.1.1",
    "@expo/vector-icons": "^6.2.1",
    "babel-preset-expo": "^4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "expo": "^25.0.0",
    "i18next": "^10.2.1",
    "immutable": "^4.0.0-rc.9",
    "lodash": "^4.17.5",
    "luxon": "^0.5.6",
    "moment": "^2.20.1",
    "native-base": "^2.3.3",
    "node-fetch": "^2.1.2",
    "react": "16.2.0",
    "react-dom": "^16.2.0",
    "react-i18next": "^7.1.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-25.0.0.tar.gz",
    "react-native-modal": "^5.0.0",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-swipeable": "^0.6.0",
    "react-native-xml2js": "^1.0.3",
    "react-navigation": "^1.0.0-beta.27",
    "react-navigation-redux-helpers": "^1.0.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "semver": "^5.5.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.60.1",
    "flow-coverage-report": "^0.4.0",
    "immutable-devtools": "0.0.7",
    "jest": "^21.2.1",
    "jest-expo": "^25.0.0",
    "react-test-renderer": "^16.0.0",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.5.1"
  }
}
